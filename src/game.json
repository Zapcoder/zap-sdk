{
    "name": "Zap test game",
    "assets": {
        "img": {
            "megusta": "/assets/img/megusta.png",
            "scenery": {
                "background": "/assets/img/scenery/background.png",
                "groundGrass": "/assets/img/scenery/groundGrass.png",
                "rockGrass": "/assets/img/scenery/rockGrass.png"
            },
            "planes": {
                "planeRed1": "/assets/img/planes/planeRed1.png"
            }
        },
        "audio": {},
        "fonts": {
            "AFolder": {
                "desyrel": {
                    "image": "/assets/fonts/AFolder/desyrel.png",
                    "data": "/assets/fonts/AFolder/desyrel.xml"
                }
            }
        }
    },
    "objects": {
        "plane": {
            "exposed": {
                "name": "Plane",
                "Sprite": {
                    "img": "planes.planeRed1"
                },
                "behaviours": {
                    "Up Force": {
                        "value": "this.components.control.upwardsAcc",
                        "type": {
                            "name": "range",
                            "min": 1000,
                            "max": 4000
                        }
                    },
                    "Gravity": {
                        "value": "this.components.grav.gravity",
                        "type": {
                            "name": "range",
                            "min": 200,
                            "max": 3000
                        }
                    }
                }
            },
            "components": [
                {
                    "img": "planes.planeRed1",
                    "group": "plane",
                    "outOfBoundsKill": true,
                    "collideBounds": true,
                    "type": "Zapcoder.Entities.Components.Sprite"
                },
                {
                    "upwardsAcc": "2000",
                    "id": "control",
                    "type": "Demo.Input.UpwardsForceController"
                },
                {
                    "gravity": "1000",
                    "id": "grav",
                    "type": "Demo.Physics.Gravity"
                },
                {
                    "type": "Demo.PlayerCollision"
                },
                {
                    "type": "Demo.SmokeEmitter"
                }
            ]
        },
        "background": {
            "exposed": {
                "name": "World",
                "Sprite": {
                    "img": "scenery.background"
                },
                "behaviours": {
                    "Scroll Speed": {
                        "value": "this.components.bg.speed",
                        "type": {
                            "name": "range",
                            "min": -3000,
                            "max": -50
                        }
                    }
                }
            },
            "components": [
                {
                    "id": "bg",
                    "image": "scenery.background",
                    "speed": "-100",
                    "group": "background",
                    "type": "Demo.Graphics.ScrollingSprite"
                }
            ]
        },
        "ground": {
            "components": [
                {
                    "image": "scenery.groundGrass",
                    "speed": "-200",
                    "group": "ground",
                    "type": "Demo.Graphics.ScrollingSprite"
                }
            ]
        },
        "worldManager": {
            "global": {
                "speed": -300
            },
            "components": [
                {
                    "ground": "ground",
                    "type": "Demo.Managers.GroundManager"
                },
                {
                    "image": "scenery.rockGrass",
                    "group": "gate",
                    "space": 300,
                    "frequency": 5,
                    "type": "Demo.Managers.GateManager"
                }
            ]
        }
    },
    "states": {
        "game": {
            "objects": [
                {
                    "type": "background",
                    "pos": {
                        "x": 0,
                        "y": 0
                    }
                },
                {
                    "type": "plane",
                    "pos": {
                        "x": "left",
                        "y": "middle"
                    }
                },
                {
                    "type": "worldManager"
                },
                {
                    "type": "ScoreDisplay",
                    "style": {
                        "font": "36px Desyrel"
                    },
                    "pos": {
                        "x": "center",
                        "y": 5
                    }
                }
            ],
            "groups": [
                "plane",
                "gate",
                "ground",
                "goal"
            ],
            "collisions": [
                {
                    "groups": [
                        "plane",
                        "ground"
                    ],
                    "type": "overlap"
                },
                {
                    "groups": [
                        "plane",
                        "goal"
                    ],
                    "type": "overlap"
                },
                {
                    "groups": [
                        "plane",
                        "gate"
                    ],
                    "type": "overlap"
                }
            ]
        }
    },
    "start": "game"
}